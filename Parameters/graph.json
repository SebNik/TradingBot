{
  "dpi": 200,
  "screen_x": 14,
  "screen_y": 8,
  "title": {
    "label": "Transactions vs. Price",
    "fontsize": 20
  },
  "legend": {
    "fontsize": 12,
    "shadow": "True",
    "fancybox": "True",
    "edgecolor": "black"
  },
  "grid_major":
  {
    "which": "major",
    "linestyle": "--",
    "color": "gray",
    "linewidth": 2,
    "alpha": 0.7
  },
  "grid_minor":
  {
    "which": "minor",
    "linestyle": "--",
    "color": "gray",
    "linewidth": 1,
    "alpha": 0.2
  },
  "x_label":
  {
    "label": "Date",
    "fontsize": 13
  },
  "y_label":
  {
    "label": "Stock-Price",
    "fontsize": 13
  },
  "x_ticks":
  {
    "rotation": 30
  },
  "x_ticks_cal":
  {
    "minor_factor": 0.02,
    "major_factor": 0.08
  },
  "line1": {
    "label": "Stock-Price",
    "color": "b",
    "linewidth": "3",
    "clip_on": "False"
  },
  "scatter_buy":
  {
    "c": "g",
    "label": "Buy-Points",
    "marker": "X",
    "edgecolors" :"black",
    "s": 125
  },
  "scatter_sell":
  {
    "c": "r",
    "label": "Sell-Points",
    "marker": "X",
    "edgecolors" :"black",
    "s": 125
  }
}